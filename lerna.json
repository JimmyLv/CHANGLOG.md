{
  "version": "1.5.1",
  "packages": [
    "packages/*"
  ],
  "command": {
    "version": {
      "allowBranch": "master",
      "message": "chore(release): publish %s"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.com/"
  },
  "npmClient": "yarn",
  "useWorkspaces": true
}
